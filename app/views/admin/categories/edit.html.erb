<h1><%= t "views.categories.edit.edit_category" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for [:admin, @category] do |f| %>
      <%= render "shared/error_messages", object: @category %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <br/>
      <%= f.submit t("views.categories.edit.edit_category"),
        class: "btn btn-primary" %>
    <% end %>
  <div>
</div>
