<% provide :title, t("user.title") %>
<h1><%= t "user.title" %></h1>
<%= will_paginate %>
<div  class="col-md-6 col-md-offset-3">
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= gravatar_for user, size: 50 %>
        <%= link_to user.name, user %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>
</div>
